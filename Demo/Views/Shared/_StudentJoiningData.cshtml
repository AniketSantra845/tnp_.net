@model List<StudentApplication>

@foreach (var item in Model)
{
    <p id="dispText">
        Minimum Stipend: @item.min_stipend
    </p>
    <p id="dispText">
        Maximum Stipend: @item.max_stipend
    </p>
    <p id="dispText">
        Minimum Salary: @item.min_salary
    </p>
    <p id="dispText">
        Maximum Salary: @item.max_salary
    </p>
    <p id="dispText">
        <b>Actual date of joining: </b>
            @{string dateString = item.date_of_joining;
            DateTime date = DateTime.Parse(dateString);

            string convertedDate = date.ToString("dd MMMM, yyyy");
            Console.WriteLine(convertedDate);
            }
            @convertedDate
       
    </p>
}


